<h2> Login! </h2>

<%= form_for :session, url: sessions_path do |f| %>
  <%= f.label :email %>
  <%= f.text_field :email %>

  <%= f.label :password %>
  <%= f.password_field :password %>

  <%= f.submit "Login", class: "btn btn-large btn-primary" %>

<% end %>

<p>Haven't signed up yet? <%= link_to "Sign up!", signup_path %></p>


